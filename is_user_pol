      COLOR [$color]
      WIDTH [$outline_width]
      OPACITY [$opacity]
    END # STYLE

     STYLE
        COLOR [$hatch_color]
        SIZE 16
        SYMBOL "backslash"
      END # STYLE
    END # CLASS
    
    
    CLASS  # Полигон с контуром 
#    NAME "Графический примитив - полигон"
    NAME "8"
     EXPRESSION (("[classif_id]" = "1000000032") AND  ("[$type]" = "polygon") AND ("[$color]" = "#ncolor") AND ("[$hatch_color]" = "#ncolor") AND  ("[$outline]" = "01") )
    # Берём только объекты, относящиеся к классификатору графических примитивов, являющиеся полигонами. Есть контур
    # Есть контур, без заливки.
    STYLE
      OUTLINECOLOR [$outline_color]
      WIDTH [$outline_width]
      OPACITY [$opacity]
    END # STYLE
   END # CLASS
    
     CLASS  # Пунктирный контур без заливки
#    NAME "Графический примитив - полигон"
    NAME "9"
     EXPRESSION (("[classif_id]" = "1000000032") AND  ("[$type]" = "polygon") AND ("[$color]" = "#ncolor") AND ("[$hatch_color]" = "#ncolor") AND  ("[$outline]" = "03"))
    # Берём только объекты, относящиеся к классификатору графических примитивов, являющиеся полигонами. Пунктирный контур
    # Заливки нет.
    STYLE
      OUTLINECOLOR [$outline_color]
      LINECAP BUTT
      LINEJOIN BEVEL 
      PATTERN
         10.00 3.00
      END # PATTERN
      WIDTH [$outline_width]
      OPACITY [$opacity]
   END # STYLE
  END # CLASS
  
   CLASS  # Точечный контур без заливки
#    NAME "Графический примитив - полигон"
    NAME "10"
    EXPRESSION (("[classif_id]" = "1000000032") AND  ("[$type]" = "polygon") AND ("[$color]" = "#ncolor") AND ("[$hatch_color]" = "#ncolor") AND  ("[$outline]" = "04"))
    # Берём только объекты, относящиеся к классификатору графических примитивов, являющиеся полигонами. Пунктирный контур
    # Заливки нет.
    STYLE
      OUTLINECOLOR [$outline_color]
      LINECAP BUTT
      LINEJOIN BEVEL 
      PATTERN
         3.00 3.00
      END # PATTERN
      WIDTH [$outline_width]
      OPACITY [$opacity]
   END # STYLE
  END # CLASS
  
  
   CLASS  # Штрих-пунктир контур без заливки
#    NAME "Графический примитив - полигон"
    NAME "11"
    EXPRESSION (("[classif_id]" = "1000000032") AND  ("[$type]" = "polygon") AND ("[$color]" = "#ncolor") AND ("[$hatch_color]" = "#ncolor") AND  ("[$outline]" = "05"))
    # Берём только объекты, относящиеся к классификатору графических примитивов, являющиеся полигонами. Штрих-пунктир контур
    # Заливки нет.
    STYLE
      OUTLINECOLOR [$outline_color]
      LINECAP BUTT
      LINEJOIN BEVEL 
      PATTERN
         10.00 3.00 3.00 3.00
      END # PATTERN
      WIDTH [$outline_width]
      OPACITY [$opacity]
   END # STYLE
  END # CLASS
  
     CLASS  # Штрих-штрихпунктир контур без заливки
#    NAME "Графический примитив - полигон"
    NAME "12"
    EXPRESSION (("[classif_id]" = "1000000032") AND  ("[$type]" = "polygon") AND ("[$color]" = "#ncolor") AND ("[$hatch_color]" = "#ncolor") AND  ("[$outline]" = "06"))
    # Берём только объекты, относящиеся к классификатору графических примитивов, являющиеся полигонами. Штрих-пунктир контур
    # Заливки нет.
    STYLE
      OUTLINECOLOR [$outline_color]
      LINECAP BUTT
      LINEJOIN BEVEL 
      PATTERN
         10.00 3.00 3.00 3.00 3.00 3.00
      END # PATTERN
      WIDTH [$outline_width]
      OPACITY [$opacity]
   END # STYLE
  END # CLASS



