    CLASS  # контур пунктир + заливка + без штриховки
#    NAME "Графический примитив - полигон"
    NAME "1211"
     EXPRESSION (("[classif_id]" = "1000000032") AND  ("[$type]" = "polygon") AND ("[$color]" != "#ncolor") AND ("[$hatch_color]" = "#ncolor") AND  ("[$outline]" = "02") )
    STYLE
