    END # STYLE

     STYLE
        COLOR [$hatch_color]
        SIZE 16
        SYMBOL "slash"
      END # STYLE
    END # CLASS
    
          CLASS  # Штриховка полигонов backslash
#    NAME "Графический примитив - полигон"
    NAME "7"
   EXPRESSION (("[classif_id]" = "1000000032") AND  ("[$type]" = "polygon") AND ("[$color]" != "#ncolor") AND  ("[$hatch_color]" != "#ncolor") AND  ("[$hatch]" = "05"))
    # Берём только объекты, относящиеся к классификатору графических примитивов, являющиеся полигонами. 
    # Есть Штриховка.
